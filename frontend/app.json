{
  "expo": {
    "name": "Todo Task",
    "slug": "todo-task-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#1e1b4b"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.todotask.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#1e1b4b"
      },
      "package": "com.todotask.app",
      "permissions": [
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "SCHEDULE_EXACT_ALARM"
      ]
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "color": "#6366f1"
        }
      ],
      "expo-router",
      "expo-secure-store",
      "expo-font",
      "expo-audio"
    ],
    "scheme": "todotask"
  }
}
